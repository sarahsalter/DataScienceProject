# DATE CREATED: 9/28/2016
# PURPOSE: To load in all the main 2015 'FARS' datasets from US Department of Transportation
library(foreign)
library(haven)

#(1): Accident Data-- crash characteristics (1 record per crash)
accident_data2015 = read_sas("~/Desktop/DataScience/TrafficProj/2015_Files/Raw_Data/National/SAS_Files/FARS2015NationalSAS/accident.sas7bdat")
summary(accident_data2015)
View(accident_data2015)

#(2): Cevent Data-- chronological sequence of events (1 record per event)
cevent_data2015 = read_sas("~/Desktop/DataScience/TrafficProj/2015_Files/Raw_Data/National/SAS_Files/FARS2015NationalSAS/cevent.sas7bdat")
summary(cevent_data2015)
View(cevent_data2015)

